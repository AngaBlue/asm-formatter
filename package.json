{
  "name": "asm-formatter",
  "displayName": "ASM Formatter",
  "version": "1.0.2",
  "publisher": "AngaBlue",
  "categories": [
    "Formatters"
  ],
  "activationEvents": [
    "onLanguage:mips"
  ],
  "description": "A formatter extension for VSCode supporting the MIPS assembly language.",
  "main": "build/index.js",
  "contributes": {
    "languages": [
      {
        "id": "MIPS Assembly",
        "aliases": [
          "MIPS"
        ],
        "extensions": [
          "asm"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc",
    "watch": "tsc -watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . --ext js,jsx,ts,tsx --fix"
  },
  "engines": {
    "vscode": "^1.66.0"
  },
  "keywords": [
    "vscode",
    "formatter"
  ],
  "author": "AngaBlue <contact@anga.blue>",
  "license": "LGPL-3.0-or-later",
  "devDependencies": {
    "@angablue/eslint-config": "^1.3.3",
    "@types/node": "^17.0.30",
    "@types/vscode": "^1.66.0",
    "eslint": "^8.14.0",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AngaBlue/asm-formatter"
  }
}
