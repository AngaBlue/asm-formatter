{
  "name": "asm-formatter",
  "version": "1.0.0",
  "publisher": "AngaBlue",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:mips",
    "onLanguage:asm"
  ],
  "description": "A formatter extension for VSCode supporting the MIPS assembly language.",
  "main": "build/index.js",
  "contributes": {
    "languages": [
      {
        "id": "MIPS Assembly",
        "aliases": [
          "MIPS"
        ],
        "extensions": [
          "asm"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "node ./node_modules/vscode/bin/install && tsc"
  },
  "engines": {
    "vscode": "^1.6.0"
  },
  "keywords": [
    "vscode",
    "formatter"
  ],
  "author": "AngaBlue <contact@anga.blue>",
  "license": "LGPL-3.0-or-later",
  "devDependencies": {
    "@angablue/eslint-config": "^1.3.3",
    "@types/node": "^17.0.30",
    "@types/vscode": "^1.66.0",
    "eslint": "^8.14.0",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4"
  }
}
